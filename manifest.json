  "permissions": [
    "storage",
    "tabs"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.svg"
  },
  "options_ui": {
    "page": "options.html"
  }
}
